{
    "name": "franzliedke/auth-fluxbb",
    "description": "A package for external authentication with FluxBB 1.5.",
    "keywords": ["fluxbb", "authentication", "laravel"],
    "license": "MIT",
    "authors": [
        {
            "name": "Franz Liedke",
            "email": "franz@develophp.org"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "illuminate/support": "~4",
        "illuminate/auth": "~4",
        "symfony/security": "~2.4"
    },
    "require-dev": {
        "mockery/mockery": "~0.9",
        "phpunit/phpunit": "~4.0"
    },
    "autoload": {
        "psr-0": {
            "FranzLiedke\\AuthFluxBB": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
